{
  "version": 2,
  "alias": ["gonads.preview.machups.com"],
  "regions": ["sfo1"],
  "framework": "nextjs",
  "buildCommand": "pnpm build",
  "build": {
    "env": {
      "NEXT_PUBLIC_BRAND_NAME": "GONADS",
      "NEXT_PUBLIC_PREVIEW_MODE": "true"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
