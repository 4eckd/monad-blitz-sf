# MachLabs Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# Jira Integration
JIRA_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-jira-api-token-here

# Cloudflare MCP Integration
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token-here
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id

# GitHub (for local development)
GITHUB_TOKEN=your-github-personal-access-token

# AI Model Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key
OPENAI_API_KEY=your-openai-api-key

# Modal.com Configuration (for GPU-accelerated brand generation)
MODAL_API_URL=https://your-app.modal.run
MODAL_API_KEY=your-modal-api-key

# Web3 Configuration (for development/testing)
ALCHEMY_API_KEY=your-alchemy-api-key
INFURA_PROJECT_ID=your-infura-project-id
PRIVATE_KEY=your-wallet-private-key-for-testing

# Application Configuration
NODE_ENV=development
PORT=3000
DATABASE_URL=postgresql://user:password@localhost:5432/machlab

# Vercel (if using Vercel deployment)
VERCEL_TOKEN=your-vercel-token